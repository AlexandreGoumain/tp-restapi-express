        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "drizzle-orm": "^0.44.1",
        "express": "^5.1.0",
        "jsonwebtoken": "^9.0.2",
        "multer": "^2.0.1",
        "pg": "^8.16.0",
